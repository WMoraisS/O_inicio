{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "B.O.M",
    "description": "Bibliography Of Material",
    "author": "Will",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    

    "browser_action": {
        "default_title": "B.O.M",
        "default_popup": "index.html",
        "default_icon": "icon/icon.png"
        
    }
    
}
